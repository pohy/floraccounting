{
  "name": "floraccounting",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.5",
    "express": "^4.15.3",
    "mongodb": "^3.0.7",
    "normalize.css": "^8.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/mongodb": "^3.0.17",
    "@types/node": "^10.0.3",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/uuid": "^3.4.3",
    "gh-pages": "^1.0.0",
    "nodemon": "^1.17.3",
    "parcel-bundler": "^1.7.1",
    "prettier": "^1.12.1",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/src/server/",
    "start-dev": "parcel --port 3000 src/client/public/index.html",
    "build-client": "parcel build -d ./dist/src/client/ --no-minify src/client/public/index.html",
    "build-server": "npx tsc -p src/server/",
    "build": "npm run build-client && npm run build-server",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "serve": "cross-env NODE_ENV=production node src/server",
    "server": "nodemon",
    "prettier": "prettier --config .prettierrc --write src/**/*.{js,jsx,ts,tsx,css}"
  },
  "homepage": "https://floraccounting.pohy.eu"
}
