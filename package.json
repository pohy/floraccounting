{
  "name": "floraccounting",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.3",
    "cross-env": "^5.1.6",
    "dotenv": "^5.0.1",
    "express": "^4.15.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.0.8",
    "node-fetch": "^2.1.2",
    "normalize.css": "^8.0.0",
    "path-to-regexp": "^2.2.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "0.0.40",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/jwt-decode": "^2.2.1",
    "@types/mongodb": "^3.0.18",
    "@types/node": "^10.1.2",
    "@types/node-fetch": "^1.6.9",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/uuid": "^3.4.3",
    "gh-pages": "^1.0.0",
    "nodemon": "^1.17.5",
    "parcel-bundler": "^1.8.1",
    "prettier": "^1.12.1",
    "ts-node": "^6.0.5",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node --require dotenv/config ./dist/src/server/",
    "start-dev": "parcel --https --port 3000 src/client/public/index.html",
    "build-client": "node --require dotenv/config ./node_modules/parcel-bundler/lib/cli.js build -d ./dist/src/client/ --no-minify src/client/public/index.html",
    "build-server": "npx tsc -p src/server/",
    "build": "npm run build-client && npm run build-server",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "nodemon",
    "prettier": "prettier --config .prettierrc --write src/**/*.{js,jsx,ts,tsx,css}"
  },
  "homepage": "https://floraccounting.pohy.eu"
}
